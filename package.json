{
  "name": "squared-icon-theme",
  "displayName": "Squared Icon Theme",
  "description": "An icon theme with minimalistic folder designs and square, uniform file icons",
  "version": "0.1.0",
  "publisher": "glsaacke",
  "repository": {
    "type": "git",
    "url": "https://github.com/glsaacke/squared-icon-theme"
  },
  "keywords": [
    "theme",
    "icon",
    "icons",
    "square",
    "material",
    "minimal"
  ],
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "iconThemes": [
      {
        "id": "squared-icon-theme",
        "label": "Squared Icon Theme",
        "path": "./styles/icon-theme.json"
      }
    ],
    "commands": [
      {
        "command": "squared-icon-theme.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
